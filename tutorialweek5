celsius = 10
print("Celcius\t\t\tFahrenheit")
while celsius < 30:
    fahrenheit = (int(9/5 * celsius) + 32)
    celsius += 5

    print(f'{celsius}\t\t\t\t{fahrenheit}')






celsius = 5
print("Celcius\t\t\tFahrenheit")
while celsius < 30:
    celsius += 5
    fahrenheit = (int(9 / 5 * celsius) + 32)
    print(f'{celsius}\t\t\t\t{fahrenheit}')




