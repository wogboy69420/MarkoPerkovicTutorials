numEvens = 0
sumOfEvens = 0
list1 = [2, 9, 6, 7, 12]
for num in list1:
    if num % 2 == 0:
	    numEvens += 1
	    sumOfEvens += num
print(numEvens, sumOfEvens)




numOfNumbers = 0
list1 = ["three", 4, 5.7, "six", "seven", 8, 3.1416]
for item in list1:
    if isinstance(item, str):
        continue
    numOfNumbers +=1
print(numOfNumbers)







